void setup()
{
  pinMode(8,OUTPUT);
  pinMode(9,OUTPUT);
  pinMode(10,OUTPUT);
}
void loop()
{
  digitalWrite(8,HIGH);
  digitalWrite(9,HIGH);
  delay(100);
  delay(100);
  digitalWrite(8,LOW);
  digitalWrite(9,LOW);
  delay(100);
  delay(100);
  digitalWrite(9,HIGH);
  digitalWrite(10,HIGH);
  delay(100);
  delay(100);
  digitalWrite(9,LOW);
  digitalWrite(10,LOW);
  delay(100);
  delay(100);
  digitalWrite(10,HIGH);
  digitalWrite(8,HIGH);
  delay(100);
  delay(100);
  digitalWrite(10,LOW);    
  digitalWrite(8,LOW);
  delay(100);
  delay(100);
}
